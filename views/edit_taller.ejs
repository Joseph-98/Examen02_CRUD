<%- include layouts/header.ejs %>

        <div class="page-data">

            <form method="post" action="/talleres/edit/<%=data[0].id%>">
                <table cellpadding="11">
                <tr>
                    <td class="label">Latitud</td><td>: <input type="text" name="latitud" value="<%=data[0].name%>" required></td>
                 </tr>
                 <tr>
                    <td class="label" valign="top">Longitud</td>
                    <td valign="top">:
                        <textarea name="longitud" required cols="30" rows="5"><%=data[0].address%></textarea>
                    </td>
                </tr>
                <tr>
                    <td class="label">Nombre</td><td>: <input type="text" value="<%=data[0].email%>" name="nombre" required></td>
                 </tr>
                  
                 <tr>
                    <td class="label"></td>
                    <td>
                        &nbsp; <input type="submit" name="submit" value="Save">
                       <input type="button" name="cancel" value="Cancel" class="cancel" onClick="cancelAdd()">
                    </td>
                 </tr>
                </table>
            </form>

        </div>

<%- include layouts/footer.ejs %>
